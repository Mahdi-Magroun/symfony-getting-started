{% extends "main/baseAdmin.html.twig" %}
{% block body %}

<div class="container ">
    <div class="row m-3">
<div class="alert alert-success w-25 row m-5" role="alert">
    <h4 class="alert-heading">Benifits!</h4>
    <p>you have made : {{data.benifits}} dt</p>
    <hr>
    
  </div>

  <div class="alert alert-info w-25 row m-5" role="alert">
    <h4 class="alert-heading">Totale order </h4>
    <p> there is {{data.order.totale}} order</p>
    <hr>
    
  </div>

  <div class="alert alert-info w-25 row m-5" role="alert">
    <h4 class="alert-heading">Request</h4>
    <p>you have {{data.account.onHold}} Request on hold</p>
    <hr>
</div>
  </div>
  <div class="row m-3">
  <div class="alert alert-info w-25 row m-5" role="alert">
    <h4 class="alert-heading">Totale users </h4>
    <p> there is {{data.account.totale}} users</p>
    <hr>
    
  </div>
  <div class="alert alert-info w-25 row m-5" role="alert">
    <h4 class="alert-heading">Totale products </h4>
    <p> there is {{data.totaleProducts}} product </p>
    <hr>
    
  </div>
  
  <div class="alert alert-warning w-25 row m-5" role="alert">
    <h4 class="alert-heading">Totale banned uses </h4>
    <p> there is {{data.account.banned}} banned users</p>
    <hr>
</div>
</div>
  </div>




  <div class="container p-5">
    <div class="row m-3">



<div class="w-25 border border-primary col m-5" >
  <h2  class="font-monospace"> account </h2>
  <div class="w-100">
    <p  class="font-monospace">active account</p>
  <div class="progress">
  
    <div class="progress-bar bg-success" role="progressbar" style="width: {{data.account.active * 100 / data.account.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.account.active * 100 / data.account.totale}}%</div> 
  </div>
 
  <p  class="font-monospace">inactive account</p>
  <div class="progress">
    <div class="progress-bar bg-info" role="progressbar" style="width: {{data.account.inactive * 100 / data.account.totale}}%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">{{data.account.inactive * 100 / data.account.totale}}%</div> 
  </div>
 
 
  <p  class="font-monospace">on hold </p>
  <div class="progress">
    <div class="progress-bar bg-warning" role="progressbar" style="width:  {{data.account.onHold * 100 / data.account.totale}}%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"> {{data.account.onHold * 100 / data.account.totale}}%</div>  
  </div>

  <p  class="font-monospace">banned  </p>
  <div class="progress">
    <div class="progress-bar bg-danger" role="progressbar" style="width: {{data.account.banned * 100 / data.account.totale}}%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"> {{data.account.banned * 100 / data.account.totale}}%</div> 
  </div>
  
</div>
</div>





<div class="w-25 border border-primary col m-5">
    <h2  class="font-monospace"> order </h2>
    <div class="w-100">
      <p  class="font-monospace">delivered</p>
    <div class="progress">
    
      <div class="progress-bar bg-success" role="progressbar" style="width: {{data.order.totaleDelivred * 100 / data.order.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.order.totaleDelivred * 100 / data.order.totale}}%</div> 
    </div>
   
    <p  class="font-monospace">not deliverd</p>
    <div class="progress">
        <div class="progress-bar bg-warning" role="progressbar" style="width: {{data.order.totaleNotDelivred * 100 / data.order.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.order.totaleNotDelivred * 100 / data.order.totale}}%</div> 
    </div>
   
   
   <p  class="font-monospace"> prepared and not delivered </p>
    <div class="progress">
        <div class="progress-bar bg-info" role="progressbar" style="width: {{data.order.preparedNotDelivred * 100 / data.order.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.order.preparedNotDelivred * 100 / data.order.totale}}%</div> 
    </div>
  
   <p  class="font-monospace"> not prepared   </p>
    <div class="progress">
        <div class="progress-bar bg-danger" role="progressbar" style="width: {{data.order.notPrepared * 100 / data.order.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.order.notPrepared * 100 / data.order.totale}}%</div> 
    </div>
    
  </div>
  </div>






<div class="w-25 border border-primary col m-5">
    <h2  class="font-monospace"> user </h2>
    <div class="w-100">
      <p  class="font-monospace">farmer</p>
    <div class="progress">
    
      <div class="progress-bar bg-primary" role="progressbar" style="width: {{data.user.totaleFarmer * 100 / data.user.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"> {{data.user.totaleFarmer * 100 / data.user.totale}}%</div> 
    </div>
   
    <p  class="font-monospace">greengrocer</p>
    <div class="progress">
        <div class="progress-bar bg-primary" role="progressbar" style="width: {{data.user.totaleGreengrocer * 100 / data.user.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.user.totaleGreengrocer * 100 / data.user.totale}}%</div> 
    </div>
   
   
   <p  class="font-monospace"> delevery </p>
    <div class="progress">
        <div class="progress-bar bg-primary" role="progressbar" style="width: {{data.user.totaleDelevery * 100 / data.user.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.user.totaleDelevery * 100 / data.user.totale}}%</div> 
    </div>
  
   <p  class="font-monospace"> admin  </p>
    <div class="progress">
        <div class="progress-bar bg-primary" role="progressbar" style="width: {{data.user.totaleAdmin * 100 / data.user.totale}}%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">{{data.user.totaleAdmin * 100 / data.user.totale}}%</div> 
    </div>
    
  </div>
  </div>
  </div>
  </div>
  
{% endblock body %}